<#Module=mdlDemo>
'---------------------------------------
Sub Load(cmdLine)
  on error resume next

  form.height = 2280
  form.width = 3960
  form.autoredraw = true
  form.caption = "Control Power"
  form.style.titlebar = false
  form.nomovemouse = true

  form.add "command",1
  form.command(1).move 3120, 1920, 720, 300
  form.command(1).caption = "Отмена"

  with form.add("label", 10)
  	.caption = "Выключить компьютер"
  	.fontsize = 10
  	.forecolor = qbcolor(15)
  	.move 240,120
  end with

  with form.add("img", 1)
  	.picture = ResToPic("1")
  	.mousepointer=99
  	.mouseicon=form.icoinet.picture
  	.move 600,840
  end with

  with form.add("label", 1)
  	.caption = "Ждущий режим"
  	.fontsize=8
  	.fontbold=true
  	.forecolor = qbcolor(15)
  	.move 180, 1320
  end with

  with form.add("img", 2)
  	.picture = ResToPic("2")
  	.mousepointer=99
  	.mouseicon=form.icoinet.picture
  	.move 1800, 840
  end with

  with form.add("label", 2)
  	.caption = "Выключение"
  	.fontsize=8
  	.fontbold=true
  	.forecolor = qbcolor(15)
  	.move 1560, 1320
  end with

  with form.add("img", 3)
  	.picture = ResToPic("3")
  	.mousepointer=99
  	.mouseicon=form.icoinet.picture
  	.move 3000, 840
  end with

  with form.add("label", 3)
  	.caption = "Перезагрузка"
  	.fontsize=8
  	.fontbold=true
  	.forecolor = qbcolor(15)
  	.move 2760, 1320
  end with

  W = form.scalewidth / 2

  for a=0 to w
      GLine form,a,0,a,form.scaleheight,sys.gdi.getgrad(int((a/w)*255),rgb(90,125,222),rgb(150,180,245))
  next

  for a=0 to w
      GLine form,a+w,0,a+w,form.scaleheight,sys.gdi.getgrad(int((a/w)*255),rgb(150,180,245),rgb(90,125,222))
  next

  GLine form,0,0,form.scalewidth,480,rgb(0,50,150), 2

  GLine form,0,1800,form.scalewidth,2280,rgb(0,50,150), 2

  for a=0 to w
      GLine form,a,480,a,504,sys.gdi.getgrad(int((a/w)*255),rgb(48,83,181),rgb(213,230,248))
  next

  for a=0 to w
      GLine form,a+w,480,a+w,504,sys.gdi.getgrad(int((a/w)*255),rgb(213,230,248),rgb(48,83,181))
  next

  GLine form,0,0,form.scalewidth-12,form.scaleheight-12,qbcolor(0), 1
 
  form.center
  form.show
End Sub
<#Module>

<#Form=form>
'-----------------------------------------
Sub Form_Load()
    sys.api.SetWindowPos form.hwnd, -1, form.left/vbPX, form.top/vbPY, form.width/vbPX, form.height/vbPY, 0
End Sub

'-----------------------------------------
Sub Img1_Click()
  sys.power("suspend")
End Sub

'-----------------------------------------
Sub Img2_Click()
  sys.power("off")
End Sub

'-----------------------------------------
Sub Img3_Click()
  sys.power("reboot")
End Sub

'-----------------------------------------
Sub Command1_Click()
  endmf
End Sub
